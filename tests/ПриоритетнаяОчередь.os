&Тест
Процедура ПриоритетнаяОчередьСоздаетсяКомпараторЛямбда() Экспорт
	
	// Дано

	// Когда

	Результат = Новый ПриоритетнаяОчередь("Первый, Второй -> {
		|	Если Первый % 2 > Второй % 2 Тогда
		|		Возврат 1;
		|	ИначеЕсли Первый % 2 = Второй % 2 Тогда
		|		Возврат 0;
		|	Иначе
		|		Возврат -1;
		|	КонецЕсли; 
		|}"
	);

	Компаратор = Рефлектор.ПолучитьСвойство(Результат, "Компаратор");

	// Тогда
	Ожидаем.Что(Результат).ИмеетТип("ПриоритетнаяОчередь");
	Ожидаем.Что(Компаратор.Выполнить(1, 2)).Равно(Компаратор(1, 2));
	Ожидаем.Что(Компаратор.Выполнить(2, 1)).Равно(Компаратор(2, 1));
	Ожидаем.Что(Компаратор.Выполнить(2, 2)).Равно(Компаратор(2, 2));

КонецПроцедуры

